<!-- vista paarcial -->
@model ClienteModel

 <div class="text-center">
    
<table class="table table-dark table-hover ">
<thead>
        <tr>
            <th scope="col">ClienteId</th>
            <th scope="col">Nombre</th>
            <th scope="col">Apellido</th>
            <th scope="col">Dni</th>
            <th scope="col">Localidad</th>
            <th scope="col">Telefono</th>

        </tr>
    </thead>
    <tbody>
    <tr>
        <th>ClienteId:@Model.Id</th> 
    <th>Nombres:@Model.Nombre</th>
    <th>Apellido:@Model.Apellido </th>
    <th>DNI:@Model.Dni </th>
    <th>Localidad:@Model.Localidad </th>
    <th>Teléfono:@Model.Telefono </th>


    </tr>
    

    </tbody>
</table>
    

    <div>
        <a asp-action="Index" >
         <input class="btn btn-primary" type="submit" value="volver a la lista"/>
         </a>
   </div>

    
</div>

      