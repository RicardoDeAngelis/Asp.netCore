    <th>@Model.Telefono </th>


    </tr>
    

    </tbody>
</table>
    

    <div>
        <a asp-action="Index" >
         <input class="btn btn-primary" type="submit" value="volver a la lista"/>
         </a>
   </div>

    
</div>

      